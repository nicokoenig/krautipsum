{
  "name": "krautipsum",
  "version": "0.0.1",
  "description": "krautipsum, a filler text generator for traditional german words",
  "engines": {
    "node": "6.3.1"
  },
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "build": "rm -rf dist && webpack",
    "build-watch": "rm -rf dist && webpack --watch",
    "heroku-postbuild": "npm i webpack && npm run build"
  },
  "dependencies": {
    "clipboard": "^1.5.12",
    "ejs": "^2.4.1",
    "express": "^4.14.0",
    "kraut": "^0.0.2",
    "normalize.css": "^4.2.0",
    /* dev dependencies below, because of heroku build */
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.8.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  },
  "keywords": [
    "kraut",
    "filler text"
  ],
  "license": "MIT"
}
